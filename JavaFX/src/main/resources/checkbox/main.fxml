<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="checkbox.Controller">
   <children>
      <CheckBox fx:id="myCheckBox" layoutX="256.0" layoutY="263.0" mnemonicParsing="false" onAction="#change" text="ON/OFF">
         <font>
            <Font size="18.0" />
         </font>
      </CheckBox>
      <Label fx:id="myLabel" layoutX="276.0" layoutY="80.0" text="OFF">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
